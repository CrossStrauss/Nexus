<%= fields_for :web_dev_project, projectable do |w| %>
  <div class="input-div">
    <%= w.label :github_url %>
    <%= w.text_field :github_url, class: 'input-styling' %>
  </div>

  <div class="input-div">
    <%= w.label :live_url %>
    <%= w.text_field :live_url, class: 'input-styling' %>
  </div>

  <div class="input-div">
    <%= w.label :tech_stack %>
    <%= w.text_area :tech_stack, class: 'input-styling' %>
  </div>
<% end %>
